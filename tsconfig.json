{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": ".",
    "types": ["node"],
     "paths": {
       "@core/*": ["src/core/*"],
       "@components/*": ["src/core/components/*"],
       "@page/*": ["src/pages*"],
       "@constants/*": ["src/constants/*"],
       "@style/*": ["src/style/*"]
     },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
